{
  "name": "mixetl",
  "version": "1.0.0",
  "description": "A reverse ETL tool that takes clean, processed data from Google Sheets and pushes into Mixpanel.",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "npm install && npm run clean && npm run mixpanel-dependencies",
    "mixpanel-dependencies": "echo y | npx api install @mixpaneldevdocs/v3.08#260uf4wkzq3unav -l js-cjs",
    "export": "ts-node --esm export/export.ts",
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "clean": "tsc --build --clean && rm -rf ./.api",
    "mixetl": "npm run build && npm run compile && npm run main",
    "main": "node ./src/index.js",
    "js-test": "jest tests/sum.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeSeeker-io/MixETL.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeSeeker-io/MixETL/issues"
  },
  "homepage": "https://github.com/CodeSeeker-io/MixETL#readme",
  "dependencies": {
    "@api/mixpaneldevdocs": "file:.api/apis/mixpaneldevdocs",
    "@google-cloud/local-auth": "^2.1.0",
    "@types/gapi": "^0.0.43",
    "express": "^4.18.2",
    "fetch": "^1.1.0",
    "google-auth-library": "^8.7.0",
    "googleapis": "^105.0.0",
    "googleapis-common": "^6.0.3",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
